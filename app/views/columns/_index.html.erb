<table class='table'>
  <thead>
    <tr>
      <th>タイトル</th>
      <th>投稿日時</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% columns.each do |column| %>
      <tr>
        <td><h4><%= column.title %></h4></td>
        <td><%= column.created_at.strftime("%Y-%m-%d") %></td>
        <td><%= link_to "このコラムを見る", column_path(column), class: "btn btn-info" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div class="container">
  <table class='table table-hover table-inverse'>
    <thead>
      <tr>
        <th>タイトル</th>
        <th>投稿日時</th>
        <th></th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% columns.each do |column| %>
        <tr>
          <td><%= link_to column.title, column_path(column), class: "column_#{column.id}" %></td>
          <td><%= column.created_at.strftime("%Y-%m-%d") %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
